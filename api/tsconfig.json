    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "build",
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "infra",
    "__tests__"
  ]
}
